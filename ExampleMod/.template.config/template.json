{
  "$schema": "http://json.schemastore.org/template",
  "author": "Lukas Olson",
  "classifications": [
    "Console",
    "ModBase"
  ],
  "identity": "GreyblockGames.ModBase",
  "name": "VoxelSharp Mod Template",
  "shortName": "voxelsharp-mod",
  "defaultName": "VoxelSharpMod",
  "description": "A base template for creating VoxelSharp mods with predefined structure.",
  "version": "1.0.0",
  "sourceName": "VoxelSharpMod",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "symbols": {
    "ModName": {
      "type": "parameter",
      "defaultValue": "VoxelSharpMod",
      "description": "The name of your mod."
    },
    "ModId": {
      "type": "parameter",
      "defaultValue": "com.example.voxelsharpmod",
      "description": "The unique identifier for your mod."
    },
    "Author": {
      "type": "parameter",
      "defaultValue": "Your Name",
      "description": "The mod author."
    }
  },
  "primaryOutputs": [
    {
      "path": "ModBase.csproj"
    }
  ]
}
